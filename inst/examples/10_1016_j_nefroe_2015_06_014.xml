            <ce:title id="tit0005">Bibliographic references in PubMed and other search engines: <ce:italic>Errare humanum est</ce:italic>
            </ce:title>
            <ce:alt-title xml:lang="es">Referencias bibliográficas en PubMed y otros buscadores. <ce:italic>Errare humanum est</ce:italic>
            </ce:alt-title>
            <ce:author-group id="aug0005">
               <ce:author id="aut0005" author-id="S2013251415001108-74306e24b7c72978e4074dc23f2a7d04">
